{
    "name": "cakedepo/admintheme",
    "description": "CakeDepo/AdminTheme  plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeDepo\\AdminTheme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDepo\\AdminTheme\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
